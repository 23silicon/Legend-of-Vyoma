import java.awt.CardLayout;

import javax.swing.JFrame;
import javax.swing.JPanel;
public class MapThatLeadsToYouOverworld extends JFrame
{
	public MapThatLeadsToYouOverworld()
	{
		this.setBounds(10,10,100,100);
		this.setTitle("Overworld");
		
		JPanel overall = new JPanel();//main panel
		CardLayout cl = new CardLayout();//creating the card layout
		overall.setLayout(cl);// setting the panel to the card layout
		this.add(overall);
		
		this.setVisible(true);
		this.setDefaultCloseOperation(this.EXIT_ON_CLOSE);
	}

}
